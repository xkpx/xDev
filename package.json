{
	"name": "xnet",
	"author": "xkp <xkpx64@mail.ru>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview --https --host ",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"update": "pnpm upgrade --latest -i",
		"node": "node ./xbuild_node/index.js",
		"express": "node ./xEXPRESS.js",
		"deno": "deno run --allow-env --allow-read --allow-net xbuild_deno/index.js"
	},
	"type": "module",
	"pnpm": {
		"overrides": {
			"typescript": "next",
			"vite": "beta",
			"svelte": "latest",
			"tslib": "latest"
		},
		"peerDependencyRules": {
			"peerDependencyRules": {
				"allowAny": [
					"typscript",
					"vite",
					"svelte",
					"tslib"
				]
			},
			"allowedVersions": {
				"typescript": "next",
				"vite": "beta",
				"svelte": "latest",
				"tslib": "latest"
			}
		}
	},
	"dependencies": {
		"@sveltejs/adapter-node": "1.0.0-next.79",
		"@sveltejs/kit": "1.0.0-next.367",
		"dotenv": "~16.0.1",
		"express": "5.0.0-beta.1",
		"javascript-obfuscator": "~4.0.0",
		"rollup-obfuscator": "~3.0.1",
		"svelte": "~3.49.0",
		"svelte-adapter-deno": "~0.6.2",
		"svelte-preprocess": "~4.10.7",
		"vite": "3.0.0-beta.9"
	}
}